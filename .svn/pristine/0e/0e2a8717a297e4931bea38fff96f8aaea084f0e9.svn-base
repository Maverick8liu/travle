<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>添加菜单</title>
<script type="text/javascript" src="app/js/main-content.js"></script>
</head>
<body>
<form action="insertMenu" method="post">
<table id="menu">
	<tr>
		<td align="left">菜单名</td>
		<td align="left"><input  type="text" name="menuname"/></td>
	</tr>
	<tr>
		<td align="left">菜单编码</td>
		<td align="left"><input  type="text" name="menucode"/></td>
	</tr>
	<tr>
		<td align="left">父菜单编码</td>
		<td align="left"><input  type="text" name="parentmenucode"/></td>
	</tr>
	<tr>
		<td align="left">菜单路径</td>
		<td align="left"><input  type="text" name="url"/></td>
	</tr>
	<tr>
		<td align="left">菜单级别</td>
		<td align="left">
			<input  type="text" name="menulevel" />
		</td>
	</tr>
	<tr>
		<td align="left">是否有子菜单</td>
		<td align="left">
			<input  type="radio" name="isleaf" />有 
				&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			<input  type="radio" name="isleaf" checked="checked"/>无 
		</td>
	</tr>
	
	<tr>
		<td align="left">菜单状态</td>
		<td align="left">
			<input  type="radio" name="status" checked="checked"/>启用  
				&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			<input  type="radio" name="status" />禁用  
		</td>
	</tr>
	
	<tr>
		<td align="left">菜单状态</td>
		<td align="center" colspan="2">
			<input  type ="submit" value="提交"/>
				&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			<input  type="reset" />  
		</td>
	</tr>
</table>
</form>
</body>
</html>